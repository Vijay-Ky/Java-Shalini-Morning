package com.shalini.java;

public class Practice {

	public static void main(String[] args) {
		int i;
		i = 100;
		System.out.println(i);//100
		//the same variable we can able reinitialize any no. of times
		i = 200;
		System.out.println(i);
		System.out.println(i);
		System.out.println(i);
		System.out.println(i);
		i = 400;
		System.out.println(i);

	}

}
